<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coolappz.Quizz" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Quiz</name>
    <description>
    SIT313 Mobile Computing - Assignment02.
  </description>
    <author email="fmenezes@deakin.edu.au" href="https://github.com/felm84/Quiz">
    Felipe Menezes Silva
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
